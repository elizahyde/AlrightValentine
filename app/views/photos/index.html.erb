<h1>Hotties</h1>
<div class="row">
  <% @photos.each do |photo| %>
    <div class="col-lg-4">
      <div>
        <img data-src="<%= photo.image %>" src="<%= photo.image %>" alt="" class="thumbnail" width="250">
      </div>
    </div>
  <% end %>
  </div>

  <%= direct_upload_form_for @uploader do |f| %>
    <p><%= f.file_field :image %></p>
    <p><%= f.submit "Upload Image" %></p>
  <% end %>





