<h1>Hotties</h1>

  <% @photos.each do |photo| %>
  <div class="row">
    <div class="col-lg-3">
      <div class="thumbnail">
        <img data-src="<%= photo.image %>" src="<%= photo.image %>" alt="">
      </div>
    </div>
  </div>
  <% end %>








